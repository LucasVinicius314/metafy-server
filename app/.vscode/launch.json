  "version": "0.2.0",
  "configurations": [
    {
      "name": "metafy-app",
      "request": "launch",
      "type": "dart",
      "toolArgs": [
        "--dart-define",
        "API_AUTHORITY_DEV=localhost:4001",
        "--dart-define",
        "API_AUTHORITY_PROD=localhost:4001",
        "--dart-define",
        "CDN_URL=https://slug.s3-accesspoint.us-east-2.amazonaws.com"
      ]
    },
    {
      "name": "metafy-app (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "metafy-app (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    }
  ]
}
